"column":17}},{"generated":{"line":99,"column":59},"source":"src/app.js","original":{"line":41,"column":16}},{"generated":{"line":99,"column":66},"source":"src/app.js","original":{"line":41,"column":0}},{"name":"root","generated":{"line":99,"column":68},"source":"src/app.js","original":{"line":41,"column":25}},{"generated":{"line":99,"column":72},"source":"src/app.js","original":{"line":41,"column":0}}],"sources":{"src/app.js":"import React, {Component, Fragment} from 'react';\nimport ReactDOM from 'react-dom';\n\nimport io from 'socket.io-client';\n\nconst socket = io('http://localhost:3000');\nsocket.on('connect', () => {\n  console.log('client connected');\n});\n\nclass App extends Component {\n  state = {currentTime: Date.now().toString()}\n\n  componentDidMount() {\n    socket.on('tick', (data) => {\n      console.log('client got tick', data);\n      this.setState({currentTime: data.currentTime})\n    })\n  }\n\n  sendClick = () => {\n    console.log('client clicked', socket.id);\n    socket.emit('send-click');\n  }\n\n  render() {\n    return <Fragment>\n      <h1>My Socket IO App</h1>\n      <p>Welcome!</p>\n      <p> \n        <button onClick={this.sendClick}>Click here!</button>\n      </p>\n      <p>\n        The current time is: {this.state.currentTime}\n      </p>\n    </Fragment>;\n  }\n}\n\nconst root = document.getElementById('root');\nReactDOM.render(<App />, root);"},"lineCount":null}},"hash":"25775e0b092d2e21885a5e55eab0839b","cacheData":{"env":{}}}